<!DOCTYPE html>
<html>
<head>
  <title>FeedMe</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<section class="content">
  <section id="flashes">
    <section id="error-sec"></section>
    <section id="notice-sec"></section>
  </section>


  <section id="sidebar-sec">
  </section>

  <main class="display">
    <%= yield %>
  </main>
  <section class="progressbar-container">
    <div id="progressbar"></div>
  </section>

  <section id="modal-sec">
  </section>

</section>

</body>
</html>
